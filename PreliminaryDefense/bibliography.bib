  File                     = {:E$\backslash$:/Work/3DPrintingJorge/LIteratureReview/2012 - Hildebrand - crdbrd Shape Fabrication by Sliding Planar Slices.pdf:pdf},
  ISBN                     = {1467-8659},
  ISSN                     = {0167-7055},
  Keywords                 = {abstraction,manufacturing techiniques},
  Mendeley-tags            = {abstraction,manufacturing techiniques},
  Url                      = {http://dx.doi.org/10.1111/j.1467-8659.2012.03037.x$\backslash$nhttp://graphics.ethz.ch/{~}bickelb/downloads/crdbrd.pdf$\backslash$nhttp://onlinelibrary.wiley.com/doi/10.1111/j.1467-8659.2012.03037.x/abstract;jsessionid=11BA11805594CB8C60F389E47A218E58.d03t01}
